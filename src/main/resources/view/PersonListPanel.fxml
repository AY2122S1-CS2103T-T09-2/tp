<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="100.0" prefWidth="200.0" />
   <SplitPane dividerPositions="0.5" minHeight="-Infinity" minWidth="-Infinity" prefWidth="200.0">
     <items>
         <VBox>
            <children>
               <Label fx:id="contactHeader" alignment="CENTER_RIGHT" text="Contacts" textAlignment="CENTER" />
              <ListView fx:id="personListView" />
            </children>
         </VBox>
         <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
               <Label fx:id="favourites" alignment="CENTER_RIGHT" text="Favourites" textAlignment="CENTER" />
               <ListView />
            </children>
         </VBox>
     </items>
   </SplitPane>
</VBox>
